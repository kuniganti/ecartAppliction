<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="loginService" class="com.ecart.services.LoginService"></bean>
	<bean id="userService" class="com.ecart.services.UserService"></bean>
	<bean id="loginHelper" class="com.ecart.helpers.LoginHelper"></bean>
	<bean id="userHelper" class="com.ecart.helpers.UserHelper"></bean>
	<bean id="applicationContextProvider" class="com.ecart.utils.SpringApplicationContextProvider" />
	<bean id="passwordManager" class="com.ecart.utils.PasswordManager"></bean>
	</beans>